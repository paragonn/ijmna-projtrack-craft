{
  "name": "paragonn/craft-ijm-project-tracker-custom-plugin",
  "description": "Custom Plugin for IJM Project tracker",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.2.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "paragonn\\IjmProjectTrackerCustomPlugin\\": "src/"
    }
  },
  "extra": {
    "handle": "_ijm-project-tracker-custom-plugin",
    "name": "IJM Project Tracker Custom Plugin",
    "developer": "Shannon McMillan",
    "documentationUrl": "https://paragonn.com"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}